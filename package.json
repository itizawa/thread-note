{
  "name": "my-next-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "npx prisma generate",
    "schema:gen": "yarn prisma generate --schema=./prisma/schema.prisma",
    "migrate:gen": "yarn prisma migrate dev --create-only --schema=./prisma/schema.prisma --name",
    "migrate-for-single": "yarn prisma migrate deploy --schema=./prisma/schema.prisma",
    "migrate": "yarn migrate-for-single",
    "migrate-for-single:reset": "yarn prisma migrate reset --skip-generate --force --schema=./prisma/schema.prisma",
    "migrate:dev": "yarn migrate-for-single && DATABASE_URL=postgresql://app_user:local-password@localhost:5432/thread-note?schema=public yarn migrate-for-single",
    "migrate:dev:reset": "yarn migrate-for-single:reset && DATABASE_URL=postgresql://app_user:local-password@localhost:5432/test?schema=public yarn migrate-for-single:reset"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.4",
    "@prisma/adapter-neon": "^6.3.1",
    "@prisma/client": "^6.3.1",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.475.0",
    "next": "15.1.6",
    "next-auth": "^5.0.0-beta.25",
    "prisma": "^6.3.1",
    "react": "^19.0.0",
    "react-day-picker": "8.10.1",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.0",
    "zod": "^3.23.8",
    "zod-prisma-types": "^3.1.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@tailwindcss/postcss": "^4.0.0",
    "@types/node": "^22.0.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.5.14",
    "eslint": "^9",
    "eslint-config-next": "15.1.6",
    "postcss": "^8",
    "prisma-case-format": "2.2.1",
    "tailwindcss": "^4.0.0",
    "typescript": "^5"
  }
}
